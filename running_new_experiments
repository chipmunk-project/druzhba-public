
Simple

    python3 druzhba_run.py simple example_alus/stateful_alus/raw.alu example_alus/stateless_alus/stateless_alu.alu 2 2 1 sigcomm_configurations/simple -t 10 -c "0,1,2,3"

Marple new flow

    python3 druzhba_run.py marple_new_flow_canonicalizer_equivalent_0 example_alus/stateful_alus/pred_raw.alu example_alus/stateless_alus/stateless_alu.alu 2 2 1 sigcomm_configurations/marple_new_flow_canonicalizer_equivalent_0 -t 2 -s "{{4}}" -p "{10}" -c "0,1,2,3"

RCP

    python3 druzhba_run.py rcp exaple_alus/stateful_alus/pred_raw.alu example_alus/stateless_alus/stateless_alu.alu 4 5 3 sigcomm_configurations/rcp_pred_raw_statless_alu_4_5 -c "0,1,2,3,5,6,42,43,4,7,8,10,9,301,300" -g 2 -t 10 -p "{4, 1}"

Blue increase

    python3 druzhba_run.py example_alus/stateful_alus/pred_raw.alu example_alus/stateful_alus/stateful_alu_arith.alu 4 2 2 sigcomm_configurations/blue_increase_pred_raw_stateless_alu_arith_4_2 -c 11,21,10,12,0,3,1,2,10,2,1" -g 2 -t 5

Conga

    python3 druzhba_run.py conga eample_alus/stateful_alus/pair.alu example_alus/stateless_alus/stateless_alu.alu 1 5 1  sigcomm_configurations/conga_pair_stateless_alu_1_5 -c "0,1,2,3" -g 5 -t 4 -s "{{24, 4}}" -p "{2,32,11,29,4}"

Snap heavy hitter

    python3 druzhba_run.py snap_heavy_hitter example_alus/stateful_alus/pair.alu example_alus/stateless_alus/stateless_alu.alu 1 1 1 sigcomm_configurations/snap_heavy_hitter_pair_stateless_1_1 -g 1 -c "0,1,2,3,998,999,1000,4" -t 505 -s "{{0, 500}}"


